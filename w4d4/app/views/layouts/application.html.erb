<!DOCTYPE html>
<html>
<head>
  <title>MyMusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <p>
    <% if flash[:notices] %>
      <% flash[:notices].each do |msg| %>
        <strong><%= msg %></strong>
      <% end %>
    <% end %>
  </p>

  
  <%= yield %>



  <br><br><%= button_to "LOG OUT", session_url, method: :delete if logged_in? %>
</body>
</html>
