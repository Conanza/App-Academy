<h1>Log In</h1>

<form action="<%= sessions_url %>" method="POST">
  <label>Username
    <input type="string" name="user[username]" value="<%= @user.username %>">
  </label>

  <label>Password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Log In">
</form>
